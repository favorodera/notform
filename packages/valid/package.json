{
  "name": "valid",
  "type": "module",
  "version": "1.0.0-alpha.1",
  "description": "Valid Vue Forms",
  "author": "Favour Emeka <favorodera@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/favorodera/valid#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favorodera/valid.git"
  },
  "bugs": {
    "url": "https://github.com/favorodera/valid/issues"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "vite",
    "test": "vitest run",
    "test:watch": "vitest watch --ui",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "zod": "^4.3.5",
    "valibot": "^1.2.0",
    "yup": "^1.6.0"
  },
  "peerDependenciesMeta": {
    "valibot": {
      "optional": true
    },
    "zod": {
      "optional": true
    },
    "yup": {
      "optional": true
    }
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.6",
    "@vitest/ui": "^4.0.17",
    "@vue/eslint-config-typescript": "^14.6.0",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "playwright": "^1.57.0",
    "tsdown": "^0.18.1",
    "type-fest": "^5.4.1",
    "typescript": "^5.9.3",
    "unplugin-vue": "^7.1.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.1",
    "vue": "^3.5.25",
    "vue-tsc": "^3.2.2",
    "yup": "^1.6.0"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0",
    "dot-prop": "^10.1.0"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=24.4.0"
  },
  "keywords": [
    "valid",
    "vue",
    "form",
    "vue form",
    "validator",
    "vue form validator"
  ]
}