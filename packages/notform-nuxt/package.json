{
  "name": "notform-nuxt",
  "version": "1.0.0-alpha.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "description": "Effortless Vue Forms Validation",
  "author": "Favour Emeka <favorodera@gmail.com>",
  "license": "MIT",
  "homepage": "https://notform-docs.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favorodera/notform.git"
  },
  "bugs": {
    "url": "https://github.com/favorodera/notform/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@nuxt/kit": "^4.3.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.13.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.0",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.3.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=24.0.0"
  },
   "keywords": [
    "notform-nuxt",
    "nuxt",
    "form",
    "nuxt form",
    "validator",
    "nuxt form validator"
  ]
}