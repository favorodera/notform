{
  "name": "notform",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0-alpha.8",
  "description": "Vue Forms Without the Friction",
  "author": "Favour Emeka <favorodera@gmail.com>",
  "license": "MIT",
  "homepage": "https://notform-docs.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favorodera/notform.git"
  },
  "bugs": {
    "url": "https://github.com/favorodera/notform/issues"
  },
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest watch --ui",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "cleanup": "rm -rf dist node_modules .turbo package-lock.json"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.6",
    "@vitest/ui": "^4.0.17",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "playwright": "^1.57.0",
    "tsdown": "^0.18.1",
    "type-fest": "^5.4.1",
    "typescript": "^5.9.3",
    "unplugin-vue": "^7.1.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.1",
    "vue": "^3.5.25",
    "vue-tsc": "^3.2.2",
    "yup": "^1.7.1"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0",
    "dot-prop": "^10.1.0"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "keywords": [
    "notform",
    "vue",
    "form",
    "vue form",
    "validator",
    "vue form validator"
  ]
}