{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        ".nuxt/**",
        ".output/**"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ]
    },
    "prepublishOnly": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "postinstall": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:watch": {
      "persistent": true,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}