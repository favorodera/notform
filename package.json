{
  "name": "valid",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/valid.umd.js",
  "module": "./dist/valid.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/valid.es.js",
      "require": "./dist/valid.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "preview": "vite preview",
    "build": "rm -rf dist && rm -f node_modules/.tmp/tsconfig.app.tsbuildinfo && vite build && vue-tsc -p tsconfig.app.json",
    "test:unit": "vitest --ui --watch --browser",
    "test:e2e": "playwright test --ui",
    "typecheck": "vue-tsc --build",
    "lint": "eslint . --fix",
    "ci:test": "vitest run --browser.headless && playwright test",
    "ci:check": "pnpm typecheck && pnpm lint",
    "ci:build": "pnpm ci:check && pnpm build && pnpm ci:test",
    "ci:version": "pnpm dlx @changesets/cli version && pnpm install --no-frozen-lockfile",
    "ci:release": "pnpm ci:build && pnpm dlx @changesets/cli publish",
    "prepublishOnly": "pnpm ci:build"
  },
  "peerDependencies": {
    "vue": "^3.5.26",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@microsoft/api-extractor": "^7.55.2",
    "@playwright/test": "^1.57.0",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/eslint-plugin": "^1.6.5",
    "@vitest/ui": "4.0.17",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "type-fest": "^5.4.1",
    "typescript": "~5.9.3",
    "vite": "beta",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.1",
    "vue-tsc": "^3.2.2"
  },
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=24.4.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  },
  "dependencies": {
    "dot-prop": "^10.1.0",
    "klona": "^2.0.6"
  }
}